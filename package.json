{
  "name": "@madsb/dkfds-vue3",
  "version": "0.9.0",
  "description": "Vue 3 implementation of Det FÃ¦lles Designsystem (DKFDS) v11 - Danish Common Design System",
  "author": "Mads Bjerre",
  "license": "MIT",
  "module": "./dist/dkfds-vue3.mjs",
  "main": "./dist/dkfds-vue3.umd.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/madsb/dkfds-vue3"
  },
  "homepage": "https://github.com/madsb/dkfds-vue3#readme",
  "files": [
    "dist/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/dkfds-vue3.mjs",
      "require": "./dist/dkfds-vue3.cjs"
    },
    "./components/*": {
      "types": "./dist/components/*.d.ts",
      "import": "./dist/components/*.js"
    },
    "./composables": {
      "types": "./dist/composables/index.d.ts",
      "import": "./dist/dkfds-vue3.mjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/dkfds-vue3.mjs"
    },
    "./styles": "./dist/dkfds-vue3.css",
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "pnpm -C examples/demo run dev",
    "build": "vite build && pnpm run typings",
    "typings": "vue-tsc --declaration --emitDeclarationOnly",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src examples",
    "lint:fix": "eslint src examples --fix",
    "format": "prettier --write src examples",
    "docs:dev": "pnpm -C docs-site run dev",
    "docs:build": "pnpm -C docs-site run build",
    "docs:preview": "pnpm -C docs-site run preview",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "dkfds": "^11.0.0",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/eslint-config-typescript": "^14.2.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-vue": "^9.33.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "flush-promises": "^1.0.2",
    "globals": "^16.3.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.4.2",
    "sass": "^1.89.2",
    "terser": "^5.43.1",
    "typescript": "^5.8.3",
    "vite": "^7.0.6",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^3.2.4",
    "vitest-axe": "^1.0.0-pre.3",
    "vue": "^3.5.18",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^3.0.4"
  }
}