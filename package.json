{
  "name": "dkfds-vue3-vite",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "pnpm -r test:run",
    "test:watch": "pnpm -r test",
    "test:coverage": "pnpm -r test:coverage",
    "test:ui": "pnpm -r test:ui",
    "test:changed": "vitest run --changed",
    "dev": "pnpm -C examples/demo run dev",
    "build": "pnpm -r --filter './packages/**' run build",
    "build:utils": "pnpm -C packages/utils run build",
    "build:core": "pnpm -C packages/core run build",
    "build:extra": "pnpm -C packages/extra run build",
    "dev:utils": "pnpm -C packages/utils run build --watch",
    "dev:core": "pnpm -C packages/core run build --watch",
    "dev:extra": "pnpm -C packages/extra run build --watch",
    "lint": "npx eslint packages/** examples/**",
    "lint:fix": "npx eslint packages/** examples/** --fix",
    "prettier": "npx prettier packages/** examples/** --check",
    "prettier:fix": "npx prettier packages/** examples/** --write",
    "format": "pnpm run prettier:fix && pnpm run lint:fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "^8.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@types/node": "^22.14.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/eslint-config-typescript": "^14.2.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-vue": "^9.33.0",
    "flush-promises": "^1.0.2",
    "globals": "^16.3.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.4.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.6",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^3.2.4",
    "vitest-axe": "^1.0.0-pre.3",
    "vue": "^3.5.18",
    "vue-eslint-parser": "^9.4.3"
  }
}
