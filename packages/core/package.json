{
  "name": "@madsb/dkfds-vue3-core",
  "version": "0.9.0",
  "description": "Core components for DKFDS v11 Vue 3 implementation",
  "author": "Mads Bjerre",
  "license": "MIT",
  "module": "./dist/dkfds-vue3-core.mjs",
  "main": "./dist/dkfds-vue3-core.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/dkfds-vue3-core.mjs",
      "types": "./dist/index.d.ts"
    },
    "./src/assets/*": "./src/assets/*",
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "vite build && pnpm run typings",
    "typings": "vue-tsc --declaration --emitDeclarationOnly",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@madsb/dkfds-vue3-utils": "workspace:*"
  },
  "peerDependencies": {
    "dkfds": "^11.0.0",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "eslint": "^9.32.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-vue": "^9.33.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "sass": "^1.89.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.6",
    "vue": "^3.5.18",
    "vue-tsc": "^3.0.4"
  }
}
