{
  "name": "@madsb/dkfds-vue3-utils",
  "version": "0.9.0",
  "description": "Utility functions for DKFDS v11 Vue 3 implementation",
  "author": "Mads Bjerre",
  "license": "MIT",
  "module": "dist/dkfds-vue3-utils.mjs",
  "main": "dist/dkfds-vue3-utils.umd.js",
  "files": [
    "dist/*"
  ],
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "vite build && pnpm run typings",
    "typings": "vue-tsc --declaration --emitDeclarationOnly",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vue": "^3.5.18",
    "vue-tsc": "^3.0.4"
  }
}
